    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "google-auth-library": "^8.7.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.0.1",
    "pino": "^8.10.0",
    "pino-pretty": "^9.3.0"
  },
  "devDependencies": {
