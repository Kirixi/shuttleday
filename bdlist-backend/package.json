  "engines": {
    "npm": ">=9.0.0 <10.0.0",
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --config nodemon.json src/index.ts",
    "build": "tsc --build --clean && tsc --build",
    "test": "pnpm run startTestDb; jest; pnpm run stopTestDb",
    "test:jestonly": "jest",
    "startTestDb": "docker compose -f src/seedDb/docker-compose.yml up -d --build",
    "stopTestDb": "docker compose -f src/seedDb/docker-compose.yml down",
    "ci:test": "NODE_ENV=test jest -i"
  },
